{
  "name": "binary-codec",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.0",
  "description": "A lightweight TypeScript utility library for working with binary data.",
  "author": "lihuaabcder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lihuaabcder/binary-codec.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "build": "tsdown",
    "docs:dev": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "binary-codec": "workspace:*",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
